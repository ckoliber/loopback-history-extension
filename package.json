{
    "name": "loopback-history-extension",
    "version": "2.2.4",
    "description": "DataModels with history extension for Loopback v4",
    "keywords": [
        "loopback-extension",
        "loopback"
    ],
    "main": "index.js",
    "engines": {
        "node": ">=8.9"
    },
    "scripts": {
        "start": "nodemon index.ts --exec ts-node",
        "prebuild": "lb-tsc --outDir typings",
        "build": "parcel build --out-dir build --cache-dir build/temp/cache --target node typings/index.js",
        "postbuild": "lb-clean build/temp",
        "test": "lb-mocha --allow-console-logs \"test/**/*.test.ts\"",
        "clean": "lb-clean build typings",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/koliberr136a1/loopback-history-extension"
    },
    "author": "KoLiBer",
    "license": "MIT",
    "files": [
        "build",
        "typings",
        "index.d.ts",
        "index.js",
        "README.md"
    ],
    "dependencies": {
        "@loopback/boot": "^1.5.12",
        "@loopback/context": "^1.24.0",
        "@loopback/core": "^1.11.0",
        "@loopback/repository": "^1.15.5"
    },
    "devDependencies": {
        "@loopback/build": "^2.1.0",
        "@loopback/testlab": "^1.9.5",
        "@types/node": "^10.17.6",
        "nodemon": "^2.0.1",
        "parcel-bundler": "^1.12.4",
        "source-map-support": "^0.5.16",
        "typescript": "~3.6.3"
    }
}
